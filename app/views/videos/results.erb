<div class="container">

  <% @videos.each do |video_id| %>

  <div style="display: inline-block; float:left; padding: 5px">

    <iframe width="350" height="197" src="//www.youtube.com/embed/<%= video_id %>" frameborder="0" allowfullscreen></iframe>

  <form action="/videos/new" method="post" id="select_list<%= video_id %>">
    <input type="hidden" name="video_id" value="<%= video_id %>"/>

    <div class="row">
      <div class="col-md-8">
    <select class="form-control" name="list_options" form="select_list<%= video_id %>">
        <option value="nope">Select List</option>
        <% @lists.each do |list| %>
        <option value="<%= list.id %>"><%= list.name %></option>
        <% end %>
    </select>
  </div>
  <div class="col-md-2">
    <button class="btn btn-primary">Add</button>
  </div>
</div>
  </form>

  </div>
  <% end %>




</div>